package practiceProgram;

import org.apache.commons.collections4.bag.SynchronizedSortedBag;

public class PatternProgramtoPrintSquare {

	public static void main(String[] args) {
	int n=5;
	for(int i=1;i<=n;i++) //==>rows
		{//i=1 1<=5 true
		//			for(int j=1;j<=n;j++)//==>columnsd
				//j=1 i<=5 true
				//j=2 2<=5 true
				//j=3 3<=5 true
				//j=4 4<=5 true
				//j=5 5<=5 true
				//j=6 5<=5 terminate
			{
				System.out.print("* ");//* * * * *
			}                           //* * * * *
			//System.out.println();        //* * * * *
			} 
	System.out.println();  //* * * * * 
	}                                      //* * * * * 

}
		
		
